<h1>Listing participant_types</h1>

<table>
  <thead>
    <tr>
      <th>Label</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @participant_types.each do |participant_type| %>
      <tr>
        <td><%= participant_type.label %></td>
        <td><%= link_to 'Show', participant_type %></td>
        <td><%= link_to 'Edit', edit_participant_type_path(participant_type) %></td>
        <td><%= link_to 'Destroy', participant_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Participant type', new_participant_type_path %>
