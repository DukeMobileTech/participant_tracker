<div class="row">
<div class="col-md-12">
    <h2 class="page-title">Participant Types</h2>

<section class="widget">
<table class="table table-striped no-margin sources-table">
  <thead>
    <tr>
       <th>ID</th>
      <th>Label</th>
    </tr>
  </thead>

  <tbody>
    <% @participant_types.each do |participant_type| %>
      <tr>
        <td><%= participant_type.id %></td>
        <td><%= link_to participant_type.label, participant_type %></td>
      </tr>
    <% end %>
  </tbody>
</table>

</section>